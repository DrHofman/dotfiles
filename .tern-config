{
  "libs": [
    "underscore",
    "browser",
    "jquery",
    "ecma5",
    "ecma6"
  ],
  "loadEagerly": [
    "lib/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "lint": {},
    "angular": {},
    "requirejs": {},
    "modules": {},
    "es_modules": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  },
  "ecmaVersion": 6
}
